{
    "name": "orkhanahmadov/laravel-zip-validator",
    "description": "Laravel ZIP file content validator",
    "keywords": [
        "orkhanahmadov",
        "laravel",
        "validation",
        "zip",
        "zip validation"
    ],
    "homepage": "https://github.com/orkhanahmadov/laravel-zip-validator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-zip": "*",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\ZipValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\ZipValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orkhanahmadov\\ZipValidator\\ZipValidatorServiceProvider"
            ]
        }
    }
}
